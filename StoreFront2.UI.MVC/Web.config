    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <connectionStrings>
   <!--
    Connectoin strings MUST have these 3 things.
  Data Source - DB Server - locally this will always be .\sqlespress
  (in a live environment this will change, and will actually be a domain URL (sql553.site4now.net))
  
  Initial Catalog - the DB that you want to connect to (ex: Northwind, Books, F2071...)
  
  Integrated Security - if set to 'true', it means we're using Windows Authentication
  -if set to 'SSPI' it is using SQL Server Authentication. (typically requires a username and password)
  - Locally, we always use Windows Authentication, so this should be set to 'true'
  ON LINE 23 WE REPLACED Initial Cat with the DB we want to connect to. We Replaced SSPI with 'True'.
   -->
    
  <add name="DefaultConnection" connectionString="Data Source=.\sqlexpress;Initial Catalog=StoreFront;Integrated Security=true" providerName="System.Data.SqlClient" />
    
      <add name="StoreFrontEntities" connectionString="metadata=res://*/StoreFront2.DATA.csdl|res://*/StoreFront2.DATA.ssdl|res://*/StoreFront2.DATA.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=.\sqlexpress;initial catalog=StoreFront;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
    
  
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
